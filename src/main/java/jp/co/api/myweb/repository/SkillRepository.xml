<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.co.api.myweb.repository.SkillRepository">
	<select id="selectAll" resultType="jp.co.api.myweb.entity.SkillEntity">
		SELECT 
			skill_id
			, skill_type
			, skill_name
		FROM
			skills
		ORDER BY
			skill_type ASC
	</select>
</mapper>